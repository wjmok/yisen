<!--pages/personal/distribution-center/team-order.wxml-->
<view class="container">

  <view class="order-title-menu-box " style="width:100%;font-size:14px;background:#fa9d26;color:#fff;text-align:center">
     成交订单数:{{web_mainData.length}}笔
  </view>

 <view class='order-title-menu'>
    <view class='order-title-menu-box' style="width:100%">
      <view class="order-title-menu-box-item " style="width:25%">
        用户姓名
      </view>
      <view class="order-title-menu-box-item " style="width:15%">
        用户昵称
      </view>
      
      <view class="order-title-menu-box-item " style="width:20%">
        购买价格
      </view>
      <view class="order-title-menu-box-item " style="width:40%">
        购买时间
      </view>
    </view>
  </view>
  

  	<block wx:for="{{web_mainData}}" wx:key="id" >
	 <view style="display:flex;width:100%;height:150rpx">
	  	<view style="font-size:14px;line-height:150rpx;width:25%;text-align:center">{{item.user.username}}</view>
	  	<view style="font-size:14px;line-height:150rpx;width:15%;text-align:center">{{item.user.nickname}}</view>
	    <view style="font-size:14px;line-height:150rpx;width:20%;text-align:center">{{item.total_price}}</view>
	    <view style="font-size:14px;line-height:150rpx;width:40%;text-align:center">{{item.create_time}}</view>
	 </view>
    </block>
  

  <view bindtap="loadMore" style="margin:0 auto;font-size:20rpx;border:1rpx solid #fa9d26;border-radius: 7rpx;color: #fa9d26;text-align: center;width:150rpx;margin-top:80rpx">加载更多</view>
</view>
